                          <tr>
                            <th class="form_table_resp">ReferralName</th>
                            <th>Username</th>
                            <th>FirstName</th>
                            <th>LastName</th>
                            <th>Email</th>
                            <th>Investment Plan</th>

                          </tr>




                          <% users.forEach((user)=>{ %>
                            <tr>

                              <td>
                                <%= user.referralName %>
                              </td>

                              <td>
                                <%= user.username %>
                              </td>
                              <td>
                                <%= user.firstName %>
                              </td>
                              <td>
                                <%= user.lastName %>
                              </td>
                              <td>
                                <%= user.email %>
                              </td>

                              <td>
                                <%= user.plan %>
                              </td>

                            </tr>

                            <% }) %>
                        </table>
                        

                        <!-- </table> -->

                      </div>
                    </div>
                  </div>
                </div>

              </div>
            </div>
          </div>
